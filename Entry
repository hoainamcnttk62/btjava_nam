public class Entry {
	public static void main(String[] args) {
		IEmployee employee1 = new PartTimeEmployee("Nam, 45000, 7);
		System.out.println("ten: " + employee1.getName());
		System.out.println("luong: " + employee1.calculateSalary());

		IEmployee employee2 = new FullTimeEmployee("Hoai", 65000);
		System.out.println("ten: " + employee2.getName());
		System.out.println("luong: " + employee2.calculateSalary());
	}
}
